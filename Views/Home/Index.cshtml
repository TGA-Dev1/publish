    }

    .container {
      display: flex;
      flex: 1;
    }

    nav {
      background: linear-gradient(to bottom, #43cea2, #185a9d);
      color: white;
      width: 200px;
      padding: 20px;
    }

    nav ul {
      list-style-type: none;
      padding: 0;
    }

    nav li {
      margin: 15px 0;
    }

    main {
      flex: 1;
      background: #f8f9fa;
      padding: 20px;
    }

